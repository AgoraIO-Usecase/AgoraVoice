<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/action_sheet_sound_effect_enable_electronic_switch_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="@string/action_sheet_sound_effect_enable_electronic_voice"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/action_sheet_sound_effect_enable_electronic_switch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@id/action_sheet_sound_effect_enable_electronic_switch_text"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/action_sheet_sound_effect_electronic_key_recycler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/action_sheet_sound_effect_enable_electronic_switch_text"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/action_sheet_sound_effect_electronic_tone_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/action_sheet_sound_effect_electronic_key_recycler"
        android:text="@string/action_sheet_sound_effect_electronic_tones_hint"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/action_sheet_sound_effect_electronic_tone_recycler"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/action_sheet_sound_effect_electronic_tone_hint"/>
</RelativeLayout>
